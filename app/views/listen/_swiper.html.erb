
<%nbimages=content.images.length%>
  <div class="swiper-container swiper-initialized swiper-horizontal swiper-pointer-events swiper-backface-hidden">
            <div class="swiper-wrapper" id="swiper-wrapper-910671eb78d45f636" aria-live="polite" style="cursor: grab; transition-duration: 0ms; transform: translate3d(-655.5px, 0px, 0px);">
    <%content.images.each_with_index do |image,i|%>


<div class="swiper-slide swiper-slide-duplicate" role="group" aria-label="<%=i+1%> / <%=nbimages%>" style="width: 334.6px; margin-right: 15px;" data-swiper-slide-index="<%=i%>">
<img width="<%=image.width%>" height="<%=image.width%>" src="<%=image.name%>" class="attachment-medium size-medium" alt="<%=image.title%>" loading="lazy" sizes="(max-width: 1200px) 100vw, 1200px">					
</div>
					
	<%end%>				

     <%content.images.each_with_index do |image,i|%>


<div class="swiper-slide " role="group" aria-label="<%=i+1%> / <%=nbimages%>" style="width: 334.6px; margin-right: 15px;" data-swiper-slide-index="<%=i%>">
<img width="<%=image.width%>" height="<%=image.height%>" src="<%=image.name%>" class="attachment-medium size-medium" alt="<%=image.title%>" loading="lazy" sizes="(max-width: 1200px) 100vw, 1200px">					
</div>
					
	<%end%>	

</div>
            <div class="swiper-navs">
                    <div class="swiper-button-prev" tabindex="0" role="button" aria-label="Previous slide" aria-controls="swiper-wrapper-910671eb78d45f636"></div>
        <!-- Add Pagination -->
    
        <div class="swiper-pagination swiper-pagination-fraction swiper-pagination-horizontal"><span class="swiper-pagination-current">1</span> / <span class="swiper-pagination-total"><%=nbimages%></span></div>
        <!-- Add Arrows -->
        <div class="swiper-button-next" tabindex="0" role="button" aria-label="Next slide" aria-controls="swiper-wrapper-910671eb78d45f636"></div>
            </div>
    <span class="swiper-notification" aria-live="assertive" aria-atomic="true"></span></div>